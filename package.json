    "author": "Ajit <me@ajitkumar.dev> (ajitkumar.dev)",
    "license": "Apache-2.0",
    "devDependencies": {
        "@babel/core": "^7.23.0",
        "@babel/plugin-transform-runtime": "^7.22.15",
        "@babel/preset-env": "^7.22.20",
        "autoprefixer": "^10.4.16",
        "babel-loader": "^9.1.3",
        "cordova-android": "^12.0.1",
        "css-loader": "^6.8.1",
        "eslint": "^8.51.0",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-react": "^7.33.2",
        "jest": "^29.7.0",
        "live-server": "^1.1.0",
        "mini-css-extract-plugin": "^2.7.6",
        "native": "file:native",
        "postcss": "^8.4.27",
        "postcss-loader": "^7.3.3",
        "raw-loader": "^4.0.2",
