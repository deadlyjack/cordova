    ],
    "author": "Ajit <me@ajitkumar.dev> (ajitkumar.dev)",
    "license": "Apache-2.0",
    "devDependencies": {
        "@babel/core": "^7.18.13",
        "@babel/plugin-transform-runtime": "^7.18.10",
        "@babel/preset-env": "^7.18.10",
        "autoprefixer": "^10.4.8",
        "babel-loader": "^8.2.5",
        "cordova-android": "^10.1.2",
        "cordova-electron": "^3.1.0",
        "css-loader": "^6.7.1",
        "electron": "^20.1.1",
        "eslint": "^8.23.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.26.0",
        "jest": "^28.1.3",
        "mini-css-extract-plugin": "^2.6.1",
        "native": "file:native",
        "postcss": "^8.4.16",
        "postcss-loader": "^7.0.1",
        "raw-loader": "^4.0.2",
        "sass": "^1.54.8",
        "sass-loader": "^13.0.2",
        "style-loader": "^3.3.1",
        "terser-webpack-plugin": "^5.3.6",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0"
    },
