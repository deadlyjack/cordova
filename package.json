        "sass-loader": "^13.3.2",
        "style-loader": "^3.3.3",
        "terser-webpack-plugin": "^5.3.9",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4",
        "xml2js": "^0.6.2"
    },
