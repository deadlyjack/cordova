    "license": "Apache-2.0",
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@babel/plugin-transform-runtime": "^7.18.10",
        "@babel/preset-env": "^7.18.10",
        "autoprefixer": "^10.4.8",
        "babel-loader": "^9.1.2",
        "cordova-android": "^10.1.2",
        "cordova-electron": "^3.1.0",
        "css-loader": "^6.7.3",
        "electron": "^20.3.12",
        "eslint": "^8.34.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.27.5",
        "jest": "^28.1.3",
        "live-server": "^1.2.2",
        "mini-css-extract-plugin": "^2.7.2",
